{
  "name": "abdullahfaqeir/nova-field-date-picker",
  "description": "Field Date based on https://vcalendar.io/",
  "keywords": [
    "laravel",
    "nova",
    "date",
    "picker",
    "field",
    "v-calendar",
    "vue"
  ],
  "license": "MIT",
  "require": {
    "php": "^8.4",
    "laravel/nova": "^4.0|^5.0"
  },
  "autoload": {
    "psr-4": {
      "AbdullahFaqeir\\VCalendar\\": "src/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "AbdullahFaqeir\\VCalendar\\FieldServiceProvider"
      ]
    }
  },
  "config": {
    "sort-packages": true
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "repositories": [
    {
      "type": "composer",
      "url": "https://nova.laravel.com"
    }
  ]
}
